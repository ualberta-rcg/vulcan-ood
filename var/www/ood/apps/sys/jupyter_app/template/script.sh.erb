#!/usr/bin/env bash

cd "${HOME}"

# Source the CVMFS-based Jupyter environment (handles noexec issue)
source /cvmfs/soft.computecanada.ca/custom/python/envs/jupyterhub_node/current/bin/activate

# Optional debug: show available kernels
jupyter kernelspec list

# Launch Jupyter Lab
jupyter lab --config="${CONFIG_FILE}"


