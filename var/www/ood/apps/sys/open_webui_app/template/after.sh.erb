#!/bin/bash

echo "[after.sh.erb] Waiting for Open WebUI Server to start on ${host}:${port}..."

if wait_until_port_used "${host}:${port}" 120; then
  echo "Open WebUI Server is up on port ${port}"
else
  echo "Timed out waiting for Open WebUI to start on port ${port}"
  clean_up 1
fi
sleep 2

echo "[after.sh.erb] Open WebUI Server ready."
